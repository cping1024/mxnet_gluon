   "source": [
    "from mxnet import ndarray as nd\n",
    "from mxnet import autograd\n",
    "\n",
    "x = nd.array([[1, 2], [3, 4]])\n",
    "x.attach_grad()\n",
    "\n",
    "with autograd.record():\n",
    "    y = x  * 2\n",
    "    z = y * x\n",
    "z.backward()\n",
    "print x.grad"
   ]
